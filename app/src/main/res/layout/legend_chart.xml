<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:gravity="center_vertical|right"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <View
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:background="@drawable/circle"
        android:backgroundTint="@color/legend_selesai"/>
    <TextView
        android:id="@+id/Tlegend_selesai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Selesai"
        android:textSize="10sp"
        android:textColor="@color/black"
        android:fontFamily="@font/interregular"
        android:layout_marginStart="5dp"/>
    <View
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:background="@drawable/circle"
        android:layout_marginStart="5dp"
        android:backgroundTint="@color/legend_proses"/>
    <TextView
        android:id="@+id/Tlegend_proses"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Proses"

        android:textSize="10sp"
        android:textColor="@color/black"
        android:fontFamily="@font/interregular"
        android:layout_marginStart="5dp"/>
    <View
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:background="@drawable/circle"
        android:layout_marginStart="5dp"
        android:backgroundTint="@color/legend_rencana"/>
    <TextView
        android:id="@+id/Tlegend_rencana"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rencana"

        android:textSize="10sp"
        android:textColor="@color/black"
        android:fontFamily="@font/interregular"
        android:layout_marginStart="5dp"/>
</LinearLayout>